{
	"name": "Kicksecure",
	"version": "1.0.1",
	"author": [
		"Zoran Dori",
		"Kicksecure"
	],
	"url": "https://kicksecure.com",
	"description": "Customizations for the Kicksecure wiki",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ResourceModules": {
		"ext.Kicksecure": {
			"scripts": "BackToTopButton.js",
			"styles": "BackToTopButton.css",
			"targets": [
				"mobile",
				"desktop"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources/ext.Kicksecure",
		"remoteExtPath": "KickSecure/resources/ext.Kicksecure"
	},
	"AutoloadClasses": {
		"Kicksecure": "src/Kicksecure.php"
	},
	"Hooks": {
		"BeforePageDisplay": [
			"Kicksecure::onBeforePageDisplay"
		]
	},
	"manifest_version": 2
}
