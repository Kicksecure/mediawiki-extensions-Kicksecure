{
	"name": "Kicksecure",
	"author": [
		"Zoran Dori",
		"Kicksecure"
	],
	"url": "https://www.kicksecure.com",
	"description": "Customizations for the Kicksecure wiki",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Kicksecure\\": "src/"
	},
	"HookHandlers": {
		"KicksecureHooks": {
			"class": "MediaWiki\\Extension\\Kicksecure\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "KicksecureHooks"
	},
	"ResourceModules": {
		"ext.Kicksecure": {
			"localBasePath": "resources/ext.Kicksecure",
			"remoteExtPath": "Kicksecure/resources/ext.Kicksecure",
			"dependencies": [],
			"styles": [
				"BackToTopButton.css"
			],
			"scripts": [
				"BackToTopButton.js"
			],
			"messages": []
		}
	},
	"manifest_version": 2
}
