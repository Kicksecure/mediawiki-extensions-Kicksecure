			"localBasePath": "resources/ext.KickSecure",
			"remoteExtPath": "KickSecure/resources/ext.KickSecure",
			"dependencies": [],
			"scripts": [
				"BackToTopButton.js"
			],
			"styles": [
				"BackToTopButton.css"
			],
			"packageFiles": [],
			"messages": []
		}
	},
	"manifest_version": 2
